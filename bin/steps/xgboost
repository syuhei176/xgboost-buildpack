#!/usr/bin/env bash
# Build Path: /app/.heroku/python/

OUT_PREFIX=$1

echo "Building XGBoost..."


SOURCE_TARBALL='https://github.com/dmlc/xgboost/archive/master.zip'

curl -L $SOURCE_TARBALL -o xgboost.zip

unzip xgboost.zip

# jx
mv xgboost-master xgboost

cd xgboost
make
# make install

mkdir -p .heroku/

# Cleanup
#cd ..
# rm -fr sqlite
